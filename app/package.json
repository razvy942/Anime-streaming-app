    "name": "Razvan",
    "email": "vanos942@gmail.com",
    "url": "https://your-website.com"
  },
  "build": {
    "appId": "com.razvan.waifu-time",
    "productName": "Waifu Time",
    "copyright": "Copyright Â© 2020 ${author}",
