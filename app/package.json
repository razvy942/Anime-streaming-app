    "webtorrent": "^0.108.1"
  },
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-builder": "^22.4.1",
    "jest": "^25.4.0"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "electronTest": "jest",
    "eject": "react-scripts eject",
    "el": "electron ./",
