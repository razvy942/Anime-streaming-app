    "concurrently": "^5.1.0",
    "electron-is-dev": "^1.2.0",
    "mocha": "^7.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
